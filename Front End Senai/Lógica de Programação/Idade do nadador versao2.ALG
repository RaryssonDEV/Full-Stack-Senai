Algoritmo "idade do nadador"

Var
idade: inteiro
 nome: caractere
Inicio
escreval("Digite a idade do nadador")
leia(idade)
//idade <- 7
//nome <- "adm"
se(idade > 5) e (idade < 7) entao
   escreval("INFANTIL A")
fimSe
   se (idade >8) e (idade <11)
   escreval("INFANTIL B")
   fimSe
      se (idade >8) e (idade <11)
      escreval("INFANTIL B")
      fimSe

Fimalgoritmo